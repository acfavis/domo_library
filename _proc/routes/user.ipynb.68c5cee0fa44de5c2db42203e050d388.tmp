def generate_search_users_body_by_email(user_email_ls: list[str] # list of user emails to search.  Note:  search does not appear to be case sensitive
    ) -> dict: # body to search v1_users_search_api
    """search does not appear to be case sensitive"""

    return {
        # "showCount": true,
        # "count": false,
        "includeDeleted": False,
        "includeSupport": False,
        "limit": 200,
        "offset": 0,
        "sort": {"field": "displayName", "order": "ASC"},
        "filters": [
            {"filterType": "text", "field": "emailAddress", "text": " ".join(user_email_ls)}
        ],
    }
